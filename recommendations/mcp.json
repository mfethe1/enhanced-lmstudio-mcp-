{
  "mcpServers": {
    "lmstudio-mcp": {
      "type": "stdio",
      "command": "py",
      "args": ["-3", "-u", "C:/Users/mfeth/.mcp-servers/lmstudio-mcp/server.py"],
      "env": {
        "ALLOWED_BASE_DIR": "C:\\Users\\mfeth\\.mcp-servers\\lmstudio-mcp",
        "LM_STUDIO_URL": "http://localhost:1234",
        "LMSTUDIO_API_BASE": "http://localhost:1234/v1",
        "MODEL_NAME": "openai/gpt-oss-20b",
        "LMSTUDIO_MODEL": "openai/gpt-oss-20b",
        "ENHANCED_STORAGE": "1",
        "PYTHONUNBUFFERED": "1",
        "EXPOSE_PUBLIC_ONLY": "1",
        "PROTOCOL_VERSION": "2025-06-18",

        "ROUTER_USE_AGENT": "1",
        "ROUTER_RATE_LIMIT_TPS": "12",
        "SMART_TASK_MAX_REFINES": "5",
        "SMART_PLAN_IMMEDIATE_TIMEOUT_SEC": "150",
        "ROUTER_BG_TIMEOUT_SEC": "300",

        "PROACTIVE_RESEARCH_ENABLED": "1",

        "HTTP_CONNECT_TIMEOUT": "10",
        "HTTP_READ_TIMEOUT_SIMPLE": "60",
        "HTTP_READ_TIMEOUT_COMPLEX": "180",
        "HTTP_MAX_RETRIES": "3",
        "HTTP_BACKOFF_FACTOR": "1.5",
        "MCP_OPTIMIZER": "1",
        "METRICS_EXPORTER": "1",
        "METRICS_BIND": "127.0.0.1:9099",

        "LMSTUDIO_FUNCTION_MODEL": "openai/gpt-oss-20b",
        "TOOLCALL_DEFAULT_TEMPERATURE": "0.15",
        "TOOLCALL_TOOL_CHOICE_DEFAULT": "auto",

        "OPENAI_MODEL": "gpt-5",
        "OPENAI_BASE_URL": "https://api.openai.com/v1",
        "ANTHROPIC_MODEL": "claude-3-5-sonnet-latest",
        "ANTHROPIC_BASE_URL": "https://api.anthropic.com/v1",
        "ANTHROPIC_VERSION": "2023-06-01",
        "OVERSEER_USE_OPUS": "1",
        "ANTHROPIC_MODEL_OVERSEER": "claude-3-opus-latest",
        "ANTHROPIC_SMART_SWITCH": "1",
        "OPUS_FOR_ANALYSIS": "1",
        "OPUS_FOR_LOW_CONF": "1",
        "LOW_CONF_THRESHOLD": "0.5",
        "ANTHROPIC_MODEL_COMPLEX": "claude-3-opus-latest"
      }
    }
  }
}
